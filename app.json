{
  "expo": {
    "name": "challenge2",
    "slug": "challenge2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-logo.png",
    "scheme": "esgi-pa2",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/app-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.acme.challenge2",
      "supportsTablet": false,
      "config": {
        "googleMapsApiKey": "AIzaSyAuWKSKCJOX7T9yhC_jgz8vypAslwBJkSg"
      }
    },
    "android": {
      "package": "com.acme.challenge2",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/app-logo.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "e95b9e11-d515-4965-a1e0-2ccafc555a25"
      }
    },
    "owner": "omaziarz"
  }
}
